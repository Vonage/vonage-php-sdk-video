{
    "name": "vonage/video",
    "description": "Client package for the Vonage Video API Server operations",
    "type": "library",
    "require": {
        "vonage/client-core": "^4.0",
        "vonage/jwt": "^0.5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpspec/prophecy-phpunit": "^2.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Vonage\\Video\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VonageTest\\": "test/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Chris Tankersley",
            "email": "chris.tankersley@vonage.com"
        },
        {
            "name": "James Seconde",
            "email": "jim.seconde@vonage.com"
        }
    ],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "phpstan": "phpstan analyse -l 2 src"
    }
}
